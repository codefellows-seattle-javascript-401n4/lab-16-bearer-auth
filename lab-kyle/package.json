{
  "name": "lab-17",
  "version": "0.0.1",
  "description": "Lab-17",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=http* nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:test": "npm run lint && npm test",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db --smallfiles",
    "dboff": "killall mongod"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krichey15/lab-17-bearer-auth.git"
  },
  "author": "richey.kyle.i@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krichey15/lab-17-bearer-auth/issues"
  },
  "homepage": "https://github.com/krichey15/lab-17-bearer-auth#readme",
  "devDependencies": {
    "debug": "^3.1.0",
    "expect": "^21.2.1",
    "faker": "^4.1.0",
    "mongodb": "^2.2.33",
    "superagent": "^3.8.2"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "jest": "^21.2.1",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^4.13.0"
  }
}
